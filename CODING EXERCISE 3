package selfprog;
import java.util.Scanner;
public class sp3 {
	public static void main (String args[])
	{
		
	Scanner sc=new Scanner(System.in);
	double x,y,z;
	
	System.out.println("insert x:");
	x=sc.nextDouble();
	System.out.println("insert y:");
	y=sc.nextDouble();
	System.out.println("insert z:");
	z=sc.nextDouble();
	
		if (y!=x) 
	     System.out.println("Result : y not equal x");
		else if(y==z)
		System.out.println("result: y equal to z");
		else;
		System.out.println("result: all different number");	
	
		
		
	
	}
}
