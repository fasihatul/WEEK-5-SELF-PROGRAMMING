package selfprog;

public class sp13 {
public static void main(String[] args) {
	char ch = 'b';
	switch (ch)
	{
	 case 'a' :
	    System.out.println("ch=a");

	 case 'b' :
	    System.out.println("ch=b"); 

	 case 'c' :
	    System.out.println("ch=c");

	 default:
	   System.out.println("default");
	} 

}
}
